#include<stdio.h>
int main()
{
  int T, i, length, last_digit;
  char n[100];
  scanf("%d",&T);
  for ( i = 1; i <= T; i++)
  {
    scanf("%s",&n);
    length = strlen(n);
    last_digit = n[length-1];
    if (last_digit%2 == 0)
    {
      printf("even\n");
    }else
    {
      printf("odd\n");
    }
    
  }
  return 0;
}
